import ShapeFooter from "../components/ShapeFooter.astro";
import LanguageSwitcher from "../components/LanguageSwitcher.astro";
import DocSearch from "../components/DocSearch.astro";

const year = new Date().getFullYear();
const baseClassName = Astro.url.pathname.split("/")?.[2] || "";

// Define the list of supported languages
const supportedLanguages = ['en', 'cn', 'ja', 'ru', 'fr'];
// Get the current path
const currentPath = Astro.url.pathname;

// Regular expression to match paths starting with /en/, /cn/, /ja/, /ru/ or /fr/
const langPrefixRegex = /^\/(en|cn|ja|ru|fr)(\/|$)/;

// Function to generate URLs for different languages
const generateUrl = (lang) => {
  if (langPrefixRegex.test(currentPath)) {
    // Replace the existing language prefix
    return currentPath.replace(langPrefixRegex, `/${lang}/`);
  } else {
    // If there is no language prefix, add a new language prefix
    return `/${lang}${currentPath}`;
  }
};

// Generate URLs for different languages
let urlCN = generateUrl('cn').replace(/\/$/, ''); // Remove the trailing slash to avoid duplication
let urlJA = generateUrl('ja').replace(/\/$/, '');
let urlEN = generateUrl('en').replace(/\/$/, '');
let urlRU = generateUrl('ru').replace(/\/$/, '');
let urlFR = generateUrl('fr').replace(/\/$/, '');

// Special handling for the root path
if (currentPath === '/cn' || currentPath === '/en' || currentPath === '/ja' || currentPath === '/ru'|| currentPath === '/fr') {
  urlCN = '/cn';
  urlEN = '/';
  urlJA = '/ja';
  urlRU = '/ru';
  urlFR = '/fr';
}

// Ensure the English homepage is '/'
if (urlEN === '/en') {
  urlEN = '/';
}

const { title, description, keywords, image } = Astro.props;

const meta = {
  image: image || 'https://static-docs.nocobase.com/20250223-173129.jpg',
  title: title
      ? `${title} - NocoBase`
      : "NocoBase - Plateforme no-code & low-code open source, auto-h√©berg√©e et pilot√©e par l‚ÄôIA",
  description:
      description ||
      "NocoBase est une plateforme no-code et low-code l√©g√®re, extr√™mement extensible, open source et enrichie par l‚ÄôIA. D√©ployez-la rapidement dans votre propre infrastructure et gardez un contr√¥le total sur vos donn√©es et vos applications.",
  keywords:
      keywords ||
      "nocobase,plateforme no-code,plateforme low-code,no-code open source,low-code open source,auto-h√©berg√©,self-hosted,AI,agents IA,AI employees,no-code development,low-code development,no code tools,plateforme no-code gratuite,int√©gration no-code,no code app",
};

